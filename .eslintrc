{
  "env": {
  "browser": true,
  "es6": true,
  "commonjs": true
},
"parserOptions": {
  "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true,
      "modules": true
  },
  "sourceType": "module"
},
"extends": "eslint:recommended",
"rules": {
  "indent": [
      "error",
      4
  ],
  "quotes": [
      "error",
      "single"
  ],
  "semi": [
     0
  ],
  "no-unused-vars": [0],
  "no-console": [0]
}
}